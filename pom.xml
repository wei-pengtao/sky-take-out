        <jjwt>0.9.1</jjwt>
        <jaxb-api>2.3.1</jaxb-api>
        <poi>5.4.0</poi>
        <mysql>8.0.33</mysql>
        <spring-security>6.4.5</spring-security>
        <wechatpay>0.4.8</wechatpay>
        <minio>8.5.17</minio>
    </properties>
    <dependencies>
    </dependencies>

    <build>
        <plugins>
